	h1 locationpanel forked from scrumblr by aliasaria

	div.board-outline
		div#board
			div#board-doodles
			image#marker(src='images/marker.png')
			image#eraser(src='images/eraser.png')


			table#board-table.board-table
				tr
					td.carpoolcol
